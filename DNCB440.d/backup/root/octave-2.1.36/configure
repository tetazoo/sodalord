while test $# != 1; do
  shift
  ac_arg=$1
  case $ac_arg in
        [\\/]*.a | ?:[\\/]*.a)
            ac_exists=false
  for ac_i in $ac_cv_flibs; do
    if test x"$ac_arg" = x"$ac_i"; then
