else
  ac_arg="$ac_arg $ac_j"
                               ac_cv_flibs="$ac_cv_flibs $ac_j"
fi

          done
          ;;
        -[lLR]*)
            ac_exists=false
  for ac_i in $ac_cv_flibs; do
    if test x"$ac_arg" = x"$ac_i"; then
      ac_exists=true
      break
    fi
  done

